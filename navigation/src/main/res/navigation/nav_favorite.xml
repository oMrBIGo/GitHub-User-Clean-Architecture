<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_favorite"
    app:startDestination="@id/favoriteFragment">

    <fragment
        android:id="@+id/favoriteFragment"
        android:name="org.cn.github.favorite.FavoriteFragment">

        <action
            android:id="@+id/action_favoriteFragment_to_nav_home"
            app:destination="@+id/nav_home"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"/>

        <action
            android:id="@+id/action_favoriteFragment_to_nav_search"
            app:destination="@id/nav_search"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"/>

        <action
            android:id="@+id/action_favoriteFragment_to_nav_setting"
            app:destination="@id/nav_setting"
            app:popUpTo="@+id/nav_main"
            app:popUpToInclusive="true"/>

    </fragment>

</navigation>